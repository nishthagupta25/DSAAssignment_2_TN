#include<iostream>
using namespace std;
int main()
{   int n;
    cin>> n;
    //taking n=5 as per assignment question
    for(int i=0;i<n;i++){
        for(int j=0;j<=i;j++){
                cout<<"* ";
            }
        cout<<endl;
        }
        return 0 ;
    }
    // *
    // * *
    // * * *
    // * * * *
    // * * * * *

